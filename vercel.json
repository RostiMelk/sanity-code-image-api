{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "outputDirectory": "public" }
    }
  ],
  "routes": [
    {
      "src": "/api/image",
      "methods": ["POST"],
      "dest": "/api/serverless.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.js"
    }
  ]
}
